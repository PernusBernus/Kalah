/**
 *
 * @author administrator
 */
public class Storage {
    int howmany = 4;
    public Storage(int number) {
        howmany=number;
    }
 
    public int getAmount() {
        return howmany;
    }
    public void setAmount(int amount) {
        howmany=amount;
    }
 
 
}
